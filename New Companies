SELECT C.COMPANY_CODE, ANY_VALUE(C.FOUNDER), COUNT(DISTINCT E.LEAD_MANAGER_CODE), COUNT(DISTINCT E.SENIOR_MANAGER_CODE),
COUNT(DISTINCT E.MANAGER_CODE), COUNT(DISTINCT E.EMPLOYEE_CODE)
FROM COMPANY AS C
JOIN EMPLOYEE AS E
ON E.COMPANY_CODE = C.COMPANY_CODE
GROUP BY C.COMPANY_CODE
ORDER BY C.COMPANY_CODE;



/*instead any_value() we also use founder in group by clause*/
